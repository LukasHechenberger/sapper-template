{
  "parserOptions": {
    "ecmaVersion": 2018
  },
  "extends": ["@ls-age"],
  "plugins": ["svelte3"],
  "rules": {
    "compat/compat": "off"
  },
  "settings": {
    "svelte3/ignore-styles": true
  },
  "overrides": [
    {
      "files": ["*.svelte"],
      "rules": {
        "import/no-mutable-exports": "off",
        "import/prefer-default-export": "off"
      }
    }
  ]
}
