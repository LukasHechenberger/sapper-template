{
  "name": "TODO",
  "version": "0.0.1",
  "description": "TODO",
  "scripts": {
    "build": "run-p build:*",
    "build:sapper": "sapper build --legacy",
    "build:sass": "npm run -s sass:base -- --output-style compressed",
    "cy:open": "cypress open",
    "cy:run": "cypress run",
    "cy:run-junit": "npm run cy:run -- --reporter junit --reporter-options 'mochaFile=test/report/report.xml'",
    "dev": "run-p dev:*",
    "dev:sapper": "sapper dev",
    "dev:sass": "run-s sass:debug \"sass:debug -- --watch src/styles --recursive\"",
    "export": "sapper export --legacy",
    "format": "prettier \"**/*\" !.gitignore !.eslintignore \"!{static,__sapper__,test/report}/**/*\" --write",
    "lint": "eslint rollup.config.js src test --ext .js,.svelte",
    "sass:base": "node-sass src/styles/global.scss -o static/css --include-path node_modules",
    "sass:debug": "npm run -s sass:base -- --source-map static/css/global.css.map --source-map-contents",
    "start": "node __sapper__/build",
    "test": "run-p --race dev cy:run",
    "test:ci": "run-p --race start cy:run-junit"
  },
  "dependencies": {
    "compression": "1.7.4",
    "polka": "0.5.2",
    "sirv": "0.4.2"
  },
  "devDependencies": {
    "@babel/core": "7.4.4",
    "@babel/plugin-syntax-dynamic-import": "7.2.0",
    "@babel/plugin-transform-runtime": "7.4.4",
    "@babel/preset-env": "7.4.4",
    "@babel/runtime": "7.4.4",
    "@ls-age/eslint-config": "0.9.0",
    "cypress": "3.3.0",
    "eslint": "5.16.0",
    "eslint-plugin-svelte3": "1.2.2",
    "node-sass": "^4.12.0",
    "npm-run-all": "4.1.5",
    "prettier": "1.17.1",
    "prettier-plugin-svelte": "0.5.1",
    "rollup": "1.12.3",
    "rollup-plugin-babel": "4.3.2",
    "rollup-plugin-commonjs": "10.0.0",
    "rollup-plugin-node-resolve": "5.0.0",
    "rollup-plugin-replace": "2.2.0",
    "rollup-plugin-svelte": "5.0.3",
    "rollup-plugin-terser": "4.0.4",
    "sapper": "0.27.1",
    "svelte": "3.4.2",
    "svelte-preprocess-sass": "^0.2.0"
  },
  "renovate": {
    "extends": [
      "@ls-age:automergeDev"
    ]
  }
}
